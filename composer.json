{
    "name": "swisnl/phpstan-faker",
    "type": "phpstan-extension",
    "description": "PHPStan Reflection extension for custom Faker providers",
    "keywords": [
        "phpstan",
        "reflection",
        "faker",
        "testing"
    ],
    "homepage": "https://github.com/swisnl/phpstan-faker",
    "license": "MIT",
    "authors": [
        {
            "name": "Rolf van de Krol",
            "email": "rvandekrol@swis.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "phpstan/phpstan": "^1.0|^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.2",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Swis\\PHPStan\\Reflection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\PHPStan\\Reflection\\Tests\\": "tests"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
